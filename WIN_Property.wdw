     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAACVSnGBPDXMsWj6x6OLCmZy/7eDd8RNuL9gY1opYk+TQGdZYvQcaOnQ9ZRtNuXUCB3S82anRncrbDjh9QlpuZfM/ha251TmIhv9BUk654iI6G1XmVyhMBl05cXw/21tO13gIyuQuXCtzh/3xNurRFNVJwoHjRrIjYOX/yYcflu1IPCnKFxnPHw93cL+gWgvDHQEjVi1wDR2ja0RFg3gnNY=
        code : |1-
         // Close the window if nothing was modified
         IF WIN_Property..Modified = False THEN
         	Close(“”,False)
         END
         
         /////////////// Check the input  ////////////////
         // At least one control key
         IF (CBOX_CTRL[1] OR CBOX_CTRL[2] OR CBOX_CTRL[3]) = False THEN
         	Error(<§$0001§>)
         	RETURN
         END
         
         // target
         IF EDT_TARGET~=“” THEN
         	Error(<§$0002§>)
         	RETURN
         END
         
         // description
         IF EDT_DESC~=“” THEN
         	IF NOT Confirm(<§$0003§>,<§$0004§>) RETURN
         END
         
         // Add?
         IF nAutoID=0 THEN
         	// Duplicate of hotkey?
         	HReadSeek(HOTKEY, Combination, [CBOX_CTRL[1], CBOX_CTRL[2], CBOX_CTRL[3]], hIdentical)
         	IF HFound() THEN
         		IF YesNo(<§$0005§>,<§$0006§>)=No
         			RETURN
         		ELSE
         			HDelete(HOTKEY)
         		END
         	END
         END
         
         /////////////// Addition/Modification /////////////// 
         HOTKEY.Ctrl		= CBOX_CTRL[1]
         HOTKEY.Alt		= CBOX_CTRL[2]
         HOTKEY.Shift		= CBOX_CTRL[3]
         HOTKEY.Target		= EDT_TARGET
         HOTKEY.Description	= EDT_DESC
         HOTKEY.Key		= NoSpace(COMBO_KEY[COMBO_KEY])
         
         // Add?
         IF nAutoID=0 THEN
         	HAdd(HOTKEY)
         	
         // Modif?
         ELSE
         	HModify(HOTKEY)
         END
         
         // Delete the temporary file
         fDelete(fTempPath()+“TempIco.bmp”)
         
         Close(WIN_Property,True)

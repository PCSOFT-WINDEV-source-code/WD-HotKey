  p_codes :
   -
     code : |1+
      Procedure WIN_Property(nAutoID=0)
   -
     code : |1+
      IF nAutoID>0 THEN
      	HReadSeekFirst(HOTKEY,HOTKEYID,nAutoID)
      	IF HFound() THEN
      		CBOX_CTRL[1] 	= HOTKEY.Ctrl
      		CBOX_CTRL[2] 	= HOTKEY.Alt
      		CBOX_CTRL[3]	= HOTKEY.Shift
      		EDT_TARGET		= HOTKEY.Target
      		EDT_DESC		= HOTKEY.Description
      		nSubscript is int
      		nSubscript = ListSeek(COMBO_KEY, HOTKEY.Key)
      		IF nSubscript<>-1 THEN
      			ListSelectPlus(COMBO_KEY, nSubscript)
      		END
      		
      	ELSE
      		Error(<§$0000§>)
      		Close()
      	END
      END
